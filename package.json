    "@rematch/persist": "^2.0.0-next.4",
    "@rematch/select": "^3.0.0-next.4",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "@types/react-redux": "7.1.9",
    "@ui-kitten/components": "5.0.0",
    "@ui-kitten/eva-icons": "5.0.0",
    "@ui-kitten/moment": "5.0.0",
